<launch>

  <!--Launches rosserial to comunicate with arduino-->
  <node pkg="rosserial_python" type="serial_node.py" name="arduino_node_1">
    <param name="port" value="/dev/ttyACM0"/> <!--Desides which port to talk to arduino-->
    <param name="baud" value="115200"/>
  </node>

   <node pkg="rosserial_python" type="serial_node.py" name="arduino_node_2">
    <param name="port" value="/dev/ttyACM1"/>
    <param name="baud" value="115200"/>
  </node>

</launch>
<launch>

  <!--Launches the simple_servo_node.py-->
  <node pkg="video_visualizer" type="simple_servo_node" name="simple_servo_node" output="screen"/>


  <!--Launches rosserial to comunicate with arduino-->
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM0"/> <!--Witch port the arduino is conected to-->
    <param name="baud" value="115200"/>
  </node>

</launch>

